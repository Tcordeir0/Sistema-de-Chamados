{
    "compilerOptions": {
        "target": "ES6",
        "module": "ES6",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "./static/js",
        "rootDir": "./static/js"
    },
    "include": ["static/js/*.ts"],
    "exclude": ["node_modules"]
}
